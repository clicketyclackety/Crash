{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run Rhino 8 WIP (Mac)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build-plugin-netcore",
            "program": "/Applications/RhinoWIP.app/Contents/MacOS/Rhinoceros",
            "args": [],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console": "internalConsole",
            "env": {
                "RHINO_PLUGIN_PATH": "${workspaceFolder}/src/Crash/bin/Debug/net7.0/Crash.rhp"
            } 
        },
        {
            "name": "Run Rhino 8 WIP (Windows)",
            "type": "clr",
